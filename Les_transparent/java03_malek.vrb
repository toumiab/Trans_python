\frametitle {Algorithme tri par sélection}
  \begin{lstlisting}
public void triSel(int tab[]) {
  int indMin; // indice du plus petit element
  int elemTmp; // element temporaire
  for (int i=0 ; i<tab.length ; i++) {
    indMin = i;
    for (int j=i+1 ; j<tab.length ; j++)
      if (tab[j] < tab[indMin])
        indMin = j;
    elemTmp = tab[indMin];
    tab[indMin] = tab[i];
    tab[i] = elemTmp;
  }
}
  \end{lstlisting}
