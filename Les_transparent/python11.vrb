\begin{procedure}[H]
  \caption{insert(noeud val,noeud pere)}
  \tcp{ \ldots{} maintenant le fils de \emph{pere} est None}
  \eIf{$val < pere$} {
    $pere.filsGauche \gets val$\;
  } {
    $pere.filsDroit \gets val$\;
  }
\end{procedure}
