\frametitle{Algorithme}
  \only<1>{
\begin{algorithm2e}[H]
  \caption{Insertion dans un arbre binaire de recherche}
  \label{alg:insertABR}
  \KwIn{noeud val}
  \eIf{$arbre = \emptyset$} {
    $racine \gets val$\;
  } {
    insert(val, racine) \tcp{insère val sous la racine}
  }
\end{algorithm2e}
