  }
  \only<2>{
\begin{procedure}[H]
  \caption{insert(noeud val,noeud pere)}
  $n \gets pere$\;
  \While{$n \neq None$} {
    $pere \gets n$\;
    \eIf{$val < n$} {
      $n \gets n.filsGauche$\;
    } {
      $n \gets n.filsDroit$\;
    }
  }
  \tcp{ maintenant le fils de \emph{pere} est None}
\end{procedure}
