  %\end{center}

\begin{array}{l} 
\verb![a,b] + [c,d] = [a+c,b+d],! \\ 
\verb![a,b] - [c,d] = [a-d, b-c],!\\ 
\verb![a,b] * [c,d] = [\min(ac, ad, bc, bd),\max(ac, ad, bc, bd)],!\\ 
\verb![a,b]/[c,d] = [a,b]*[\frac{1}{d},\frac{1}{c}]$ si $0\notin [c,d].! \\
\end{array}
\end{frame}


\begin{frame}
  \frametitle{Représentation de la pile\chrono}
  Exemple : pile \only<1-2>{d'entiers}\only<3>{d'éléments}
  \begin{itemize}
  \item Choix d'une représentation interne : 
	\begin{itemize}
		\item \textcolor<2>{red}{tableau d'entiers + position du sommet de la pile}
		\item \textcolor<3>{red}{une liste + capacité autorisée de la pile : possibilité de mettre tout type d'objet} 
		\item etc
	\end{itemize}
	\pause
  \item \textcolor<2>{red}{Création d'une pile} \only<2>{: \textcolor{red}{allouer le tableau}\\} 
	       \only<2>{$\Longrightarrow$ voir le polycopié du cours} \only<3>{: \textcolor{red}{définir une liste + capacité}}
  \item Empiler, dépiler, extraire le sommet : accéder au tableau/liste
  \item Pile vide ou pleine : tester la taille du tableau/capacité de la liste
  \end{itemize}
\end{frame}

\begin{frame}
  \frametitle{Pile en Python\chrono}
  \begin{exemple}[Pile]
	\small
    \begin{semiverbatim}
class Pile(object): 

\only<2>{  \hspace{1cm}def \_\_init\_\_(self, max):

    \hspace{2cm}self.\_\_contenu = []

    \hspace{2cm}self.\_\_maxpile = max 
}

\only<3>{  \hspace{1cm}def estvide(self): 

    \hspace{2cm}return len(self.\_\_contenu)==0

 \hspace{1cm}def estpleine(self):  

    \hspace{2cm}return len(self.\_\_contenu) == \
		self.\_\_maxpile

  }
\only<4>{ \hspace{1cm}def empiler(self, val):

    \hspace{2cm}if not self.estpleine(): 

        \hspace{3cm}self.\_\_contenu.append(val)
				
    \hspace{2cm}else: 
		   
			 \hspace{3cm}raise ValueError("err : pile pleine")
  }
\only<5>{  \hspace{1cm}def depiler(self): 

    \hspace{2cm}if not self.estvide():  

        \hspace{3cm}return self.\_\_contenu.pop()

    \hspace{2cm}else:

      \hspace{3cm}return None 
    }
\only<6>{  \hspace{1cm}def lire\_sommet(self): 

    \hspace{2cm}if not self.estvide():

      \hspace{3cm}return self.\_\_contenu[-1]

    \hspace{2cm}return None 
}
     
    \end{semiverbatim}
  \end{exemple}
\end{frame}





