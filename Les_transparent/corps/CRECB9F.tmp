\begin{frame}
  \frametitle{Décorateur\chrono}
  
    \textcolor{red}{Encapsulation}
    \begin{itemize}
      \item Protéger l'accès aux variables d'instance : comment y accéder ?
      \begin{enumerate}
		    \item utilisation de méthodes \texttt{get} et \texttt{set}
			  \item \color<1->{red}{utilisation de décorateurs : remplacent get() et set()} :
		  \end{enumerate}		
		  \begin{itemize}
			  \item remplace le getteur d'un attribut par une méthode devant laquelle figure \textcolor{red}{\texttt{$@$proprety}}
	\item remplace le setteur d'un attribut par une méthode devant laquelle figure \textcolor{red}{\texttt{$@$nomAttribut.setter}}
				\end{itemize}
		     
    \end{itemize}
   
		
\end{frame}

\begin{frame}

  \frametitle{Décorateur\chrono}

      \begin{exemple}
			\begin{semiverbatim}
			\vspace{-0.5cm}
			class Rationnel(object):
			
			 \hspace{1cm}def \textcolor{red}{\twound{init}\twound}(\textcolor{blue}{self}, num, den =1):
	     
			\hspace{2cm}\textcolor{red}{self.}\textcolor<2>{red}{\_\_}\textcolor{red}{num} = num				
        
        \hspace{1cm}\textcolor[rgb]{0.5,0.5,0.5}{@property}
				
        \hspace{1cm}\textcolor{red}{def} num(\textcolor{red}{self}):

          \hspace{2cm}return self.\_\_num
          
        \hspace{1cm}\textcolor[rgb]{0.5,0.5,0.5}{@num.setter}
				
        \hspace{1cm}\textcolor{red}{def} num(\textcolor{red}{self}, num): 

          \hspace{2cm}self.\_\_num = num
					\pause
					\small{
					
					\# Exécution ------------------------
					
					r = Rationnel(2,5) \textcolor[rgb]{0,0.58,0}{\# création de l'objet r =2/5}\pause
					
					print(r.num) \pause \textcolor[rgb]{0,0.58,0}{\# => acces à num de l'objet r : affiche 2}
					
					r.num = 10   \pause \textcolor[rgb]{0,0.58,0}{\# => appel au setter : num vaut 10}
					
					print(r.num)  \pause \textcolor[rgb]{0,0.58,0}{\# => acces à num de l'objet r : affiche 10}}

					
\end{semiverbatim}


\end{exemple}
\end{frame}

\begin{frame}
  \frametitle{Constructeur\chrono}
  \begin{itemize}
  \item Rôle : créer et initialiser l'objet (\textcolor{blue}{variables d'instance})
  \item Appelé par l'\textcolor{red}{appel de la classe} : \texttt{obj = Rationnnel(1,5)}
  \item Méthode \texttt{\_\_init\_\_(self, ...)} dont le premier paramètre est \emph{self} 
  \end{itemize}

  \begin{exemple}[Constructeur]
    def \_\_init\_\_(self, num, den):

    \hspace{1cm}\textcolor{blue}{self.\_\_num} = num

    \hspace{1cm}\textcolor{blue}{self.\_\_den} = den

    
  \end{exemple}
\end{frame}

\begin{frame}
  \frametitle{Contexte d'exécution\chrono}
  \begin{itemize}
  \item Une variable = une instance = un contexte d'exécution
  \item chaque variable contient ses données propres
  \end{itemize}

  \begin{exemple}
    \begin{semiverbatim}
      x1 = Rationnel(1,2)

      x2 = Rationnel(2,3)

      print(x1.get\_numerateur())

      print(x2.get\_numerateur())

      $\Rightarrow$ 1 2
    \end{semiverbatim}
  \end{exemple}
\end{frame}

\begin{frame}
  \frametitle{L'objet \emph{self}\chrono}
  \only<1-3>{
    \begin{itemize}
    \item \emph{self} : objet représentant le contexte courant
    \item utilisable uniquement dans une méthode d'instance
		\item Utilisable quand on fait appel à une méthode d'instance depuis la classe
    \end{itemize}
    \pause
    \begin{exemple}[Utilisation de \emph{self}]
      \begin{semiverbatim}
			\vspace{-0.5cm}
       class Rationnel(object):
			
			\hspace{1cm}def \_\_init\_\_(self, n, d):
      
			\hspace{2cm}\textcolor[rgb]{1,0,0}{den = n + d}
			
      \only<2>{\hspace{2cm}\textcolor{blue}{self.\_\_num} = n}\only<3>{\hspace{2cm}\textcolor{red}{self.set\_num(n)}}

      \hspace{2cm}\textcolor{blue}{self.\_\_den} = d
        
				\pause
				\hspace{1cm}def get\_num(self):
      
        \hspace{2cm}num = 0

        \hspace{2cm}return \textcolor{red}{self.}\_\_num

      \end{semiverbatim}
    \end{exemple}}
